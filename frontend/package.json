{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "^7.0.0",
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.10.0",
    "@reduxjs/toolkit": "1.9.1",
    "axios": "0.27.2",
    "date-fns": "2.29.3",
    "formik": "2.2.9",
    "http-proxy-middleware": "2.0.6",
    "hybrid-crypto-js": "^0.2.4",
    "lodash": "^4.17.21",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-calendar": "^4.0.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-image-file-resizer": "0.4.8",
    "react-redux": "8.0.4",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "redux-thunk": "2.4.1",
    "web-vitals": "2.1.4",
    "yup": "0.32.11"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "cross-env CI=true craco test",
    "test:watch": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx src/",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "electron": "electron electron.js",
    "start:test": "concurrently \"yarn --cwd ../backend/ start:test\" \"wait-on http-get://localhost:3001/testing/ping && cross-env BROWSER=none yarn start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.2.0",
    "cypress-localstorage-commands": "^2.2.2",
    "deep-freeze": "0.0.1",
    "electron": "^21.3.1",
    "eslint": "8.22.0",
    "eslint-plugin-custom-rules": "file:./eslint",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-fail-on-console": "^3.0.2",
    "wait-on": "^7.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^@reducers(.*)$": "<rootDir>/src/reducers/$1",
      "^@utils(.*)$": "<rootDir>/src/utils/$1"
    }
  }
}